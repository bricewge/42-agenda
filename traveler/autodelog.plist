<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>autodelog</string>

    <key>ProgramArguments</key>
    <array>
      <string>$HOME/.local/bin/autodelog.js</string>
    </array>

    <key>Nice</key>
    <integer>1</integer>

    <key>StartInterval</key>
    <integer>20</integer>

    <key>OnDemand</key>
    <false/>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/autodelog.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/autodelog.out</string>

  </dict>
</plist>
